{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"lib": ["es6"],
		"sourceMap": true,
		"rootDir": "src",
		"strictNullChecks": true,
		"strictPropertyInitialization": true,
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"declaration": true
	},
	"exclude": ["node_modules", ".vscode-test", "out"]
}
